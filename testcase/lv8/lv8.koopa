decl @getint(): i32
decl @getch(): i32
decl @getarray(*i32): i32
decl @putint(i32)
decl @putch(i32)
decl @putarray(i32, *i32)
decl @starttime()
decl @stoptime()

fun @main(@a1_p: i32, @a2_p: i32): i32 {
%entry:
    @a2_1 = alloc i32
    store @a2_p, @a2_1
    @a1_1 = alloc i32
    store @a1_p, @a1_1
    ret 0
}


