fun @main(): i32 {
%entry:
    @i_1 = alloc i32
    store 0, @i_1
    jump %while_entry_0

%while_entry_0:
    %0 = load @i_1
    %1 = lt %0, 10
    br %1, %while_body_0, %end_0

%while_body_0:
    store 20, @i_1
    jump %while_entry_0

    %2 = load @i_1
    %3 = add %2, 1
    store %3, @i_1
%end_0:
